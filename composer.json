{
    "name": "denis-kisel/repeator",
    "description": "Repeator for laravel admin widgets",
    "authors": [
        {
            "name": "Denis Kisel",
            "email": "denis.kisel92@gmail.com",
            "homepage": "http://digital-elephant.com.ua"
        }
    ],
    "homepage": "https://github.com/denis-kisel/repeator",
    "keywords": ["Laravel", "Repeator"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DenisKisel\\Repeator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DenisKisel\\Repeator\\RepeatorServiceProvider"
            ],
            "aliases": {
                "Repeator": "DenisKisel\\Repeator\\Facades\\Repeator"
            }
        }
    }
}
